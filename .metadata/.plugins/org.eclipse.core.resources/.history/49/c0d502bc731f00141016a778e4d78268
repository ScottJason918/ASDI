//requiring file
var req = require("data");
var data = req.litNot;
//main Window
var mainWindow = Ti.UI.createWindow({
	backgroundColor : "#FFF"
});
//creating table
var table = Ti.UI.createTableView({
});
//alert from object
var anAlert = function(){
	this.info;
};

//making table view rows from literal notation
for(var i = 0; i < data; i++){
	var aRow = Ti.UI.createTableViewRow({
		title : data[i].name,
		desc : data[i].type,
		hasChild: true,
		hasDetail: true
	});
	table.add(aRow);
	aRow.addEventListener("click", anAlert);
};

